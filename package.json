{
  "name": "ngx-semantic-ui",
  "version": "16.0.0",
  "description": "Angular Semantic UI Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular-ex/ngx-semantic-ui/tree/main"
  },
  "bugs": {
    "url": "https://github.com/angular-ex/ngx-semantic-ui/issues"
  },
  "author": "https://github.com/angular-ex/ngx-semantic-ui/graphs/contributors",
  "keywords": [
    "angular",
    "angular-components",
    "semantic-ui",
    "no-jquery",
    "ui",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "lib:lint1": "ng lint lib",
    "lib:lint": "",
    "lib:test": "ng test lib --code-coverage --source-map false --progress false --watch false",
    "lib:compile:locales": "tsc --project projects/lib/src/behaviors/localization/tsconfig.locales.json && node ./scripts/locales",
    "lib:compile": "ng build lib && npm run lib:compile:locales",
    "lib:compile:w": "ng build lib --watch",
    "lib:compile:prod": "ng build lib --prod && npm run lib:compile:locales",
    "lib:build": "npm run lib:lint && npm run lib:compile",
    "lib:build:prod": "npm run lib:lint && npm run lib:compile:prod",
    "demo:serve": "ng serve demo --host 0.0.0.0",
    "demo:lint": "ng lint demo",
    "demo:compile": "ng build demo",
    "demo:compile:prod": "ng build demo --prod",
    "demo:build": "npm run demo:compile",
    "demo:build:prod": "npm run demo:compile:prod",
    "lint": "npm run lib:lint && npm run demo:lint",
    "test": "npm run lib:test",
    "build": "npm run lib:build && npm run demo:build",
    "build:prod": "npm run lib:build:prod && npm run demo:build:prod"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular-eslint/builder": "16.1.2",
    "@angular-eslint/eslint-plugin": "16.1.2",
    "@angular-eslint/eslint-plugin-template": "16.1.2",
    "@angular-eslint/schematics": "16.1.2",
    "@angular-eslint/template-parser": "16.1.2",
    "@angular/animations": "^16.2.4",
    "@angular/cli": "^16.2.1",
    "@angular/common": "^16.2.4",
    "@angular/compiler": "^16.2.4",
    "@angular/core": "^16.2.4",
    "@angular/forms": "^16.2.4",
    "@angular/language-service": "^16.2.4",
    "@angular/platform-browser": "^16.2.4",
    "@angular/platform-browser-dynamic": "^16.2.4",
    "@angular/router": "^16.2.4",
    "@types/extend": "^3.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^12.11.1",
    "@types/prismjs": "~1.4.18",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "bowser": "^1.7.2",
    "core-js": "^2.5.4",
    "date-fns": "2.0.0-alpha.1",
    "element-closest": "^2.0.2",
    "eslint": "^8.47.0",
    "extend": "^3.0.1",
    "fs-extra": "^5.0.0",
    "is-ua-webview": "^1.0.0",
    "jasmine-core": "~4.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.2.0",
    "popper.js": "^1.14.0",
    "prismjs": "~1.6.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "~5.1.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.13.0"
  },
  "overrides": {
    "node-sass": "6.0.1"
  }
}